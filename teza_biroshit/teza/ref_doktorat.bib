@article{fennessy_multi-locus_2016,
	title = {Multi-locus {Analyses} {Reveal} {Four} {Giraffe} {Species} {Instead} of {One}},
	volume = {26},
	issn = {0960-9822},
	url = {http://dx.doi.org/10.1016/j.cub.2016.07.036},
	doi = {10.1016/j.cub.2016.07.036},
	abstract = {Graphical Abstract Highlights d Four genetically distinct giraffe clusters suggest separation into four species d The first genetic study of giraffe using nuclear sequences and the Nubian giraffe Q1 d Rothschild's giraffe should be subsumed into the nominate Nubian giraffe d A giraffe survey genome produces valuable markers for phylogenomic analyses In Brief Fennessy et al. show that multi-locus population genetic analyses indicate four distinct giraffe species instead of one. As the most inclusive genetic analysis of giraffe relationships to date, the findings highlight the need for targeted conservation efforts of the world's tallest megafauna. SUMMARY Traditionally, one giraffe species and up to eleven subspecies have been recognized [1]; however, nine subspecies are commonly accepted [2]. Even after a century of research, the distinctness of each giraffe subspecies remains unclear, and the genetic variation across their distribution range has been incompletely explored. Recent genetic studies on mtDNA have shown reciprocal monophyly of the matrilines among seven of the nine assumed subspecies [3, 4]. More-over, until now, genetic analyses have not been applied to biparentally inherited sequence data and did not include data from all nine giraffe subspecies. We sampled natural giraffe populations from across their range in Africa, and for the first time individuals from the nominate subspecies, the Nubian giraffe, Giraffa camelopardalis camelopardalis Linnaeus 1758 [5], were included in a genetic analysis. Coales-cence-based multi-locus and population genetic ana-lyses identify at least four separate and monophyletic clades, which should be recognized as four distinct giraffe species under the genetic isolation criterion. Analyses of 190 individuals from maternal and bipa-rental markers support these findings and further sug-gest subsuming Rothschild's giraffe into the Nubian giraffe, as well as Thornicroft's giraffe into the Masai giraffe [6]. A giraffe survey genome produced valuable data from microsatellites, mobile genetic elements, and accurate divergence time estimates. Our findings provide the most inclusive analysis of giraffe relation-ships to date and show that their genetic complexity has been underestimated, highlighting the need for greater conservation efforts for the world's tallest mammal.},
	journal = {Current Biology},
	author = {Fennessy, Julian and Bidon, Tobias and Reuss, Friederike and Vamberger, Melita and Fritz, Uwe and Janke Correspondence, Axel and Kumar, Vikas and Elkan, Paul and Nilsson, Maria A and Janke, Axel},
	year = {2016},
	note = {Publisher: Elsevier Ltd
Citation Key: Fennessy2016
ISBN: 0960-9822},
	pages = {1--7}
}

