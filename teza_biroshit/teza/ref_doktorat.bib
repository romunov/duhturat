@article{ivan_using_2013,
	title = {Using simulation to compare methods for estimating density from capture-recapture data},
	volume = {94},
	issn = {00129658},
	url = {http://doi.wiley.com/10.1890/12-0102.1},
	doi = {10.1890/12-0102.1},
	abstract = {Estimation of animal density is fundamental to wildlife research and management, but estimation via mark–recapture is often complicated by lack of geographic closure of study sites. Contemporary methods for estimating density using mark–recapture data include (1) approximating the effective area sampled by an array of detectors based on the mean maximum distance moved (MMDM) by animals during the sampling session, (2) spatially explicit capture–recapture (SECR) methods that formulate the problem hierarchically with a process model for animal density and an observation model in which detection probability declines with distance from a detector, and (3) a telemetry estimator (TELEM) that uses auxiliary telemetry information to estimate the proportion of animals on the study site. We used simulation to compare relative performance (percent error) of these methods under all combinations of three levels of detection probability (0.2, 0.4, 0.6), three levels of occasions (5, 7, 10), and three levels of abundanc...},
	number = {4},
	journal = {Ecology},
	author = {Ivan, Jacob S. and White, Gary C. and Shenk, Tanya M.},
	month = apr,
	year = {2013},
	note = {Citation Key: ivan-et-al-2013},
	keywords = {Closure, Density, Geographic closure, Mean maximum distance moved, Simulation, Spatially explicit capture-recapture, Telemetry, Trapping grid},
	pages = {817--826},
