	url = {http://doi.wiley.com/10.1002/jwmg.851},
	doi = {10.1002/jwmg.851},
	abstract = {Presence-absence (detection/non-detection) data are routinely collected{\textbackslash}nin wildlife studies where identification of individuals is impossible or{\textbackslash}nimpractical and where the detection method may be able to detect only{\textbackslash}nthe presence of an individual rather than a count (e.g., track or scat{\textbackslash}nsurveys). Estimating population density from presence-absence data{\textbackslash}nusually is assumed to be difficult or impossible unless certain{\textbackslash}nrestrictive assumptions are made or supplementary information is{\textbackslash}ncollected. Recently, Chandler and Royle (2013) presented an extension of{\textbackslash}na spatially explicit capture-recapture model that estimates population{\textbackslash}ndensity from spatially replicated counts in unmarked populations. We{\textbackslash}nextended the model of Chandler and Royle (2013) to situations where only{\textbackslash}npresence-absence data can be collected. The model assumes that{\textbackslash}nindividuals can be detected at multiple sample units, producing{\textbackslash}nspatially correlated detections. A spatially explicit model of the{\textbackslash}ndetection process is then fit to the correlated detection data using{\textbackslash}nBayesian methods. We report on the performance of the model using{\textbackslash}nsimulation and illustrate its use with a practical example estimating{\textbackslash}nthe abundance and density of red foxes (Vulpes vulpes) from remote{\textbackslash}ncamera surveys in the Grampians National Park in southeastern Australia.{\textbackslash}nResults from simulations suggest the model produces unbiased estimates{\textbackslash}nof density if device spacing is less than the radial length of a typical{\textbackslash}nhome range and the number of encounter occasions is high (i.e., at least{\textbackslash}n10). Application of the model to camera detection data from foxes in the{\textbackslash}nGrampians National Park resulted in an estimated density of{\textbackslash}n0.22foxes/km(2) (95\% CI: 0.16-0.53). For this dataset, precision of the{\textbackslash}ndensity and detection parameter estimates were increased by the use of{\textbackslash}nan informative prior distribution for the home-range-scale parameter.{\textbackslash}nThe current model should apply widely to a range of sampling situations{\textbackslash}nthat result in spatially correlated detection/non-detection data such as{\textbackslash}nbait take, scat surveys, tracking stations, and chew cards, to name a{\textbackslash}nfew. (c) 2015 The Wildlife Society.},
	number = {3},
	journal = {The Journal of Wildlife Management},
	author = {Ramsey, David S L and Caley, Peter A. and Robley, Alan},
	month = apr,
	year = {2015},
	note = {Citation Key: Ramsey2015},
	keywords = {Bayesian analysis, camera traps, MCMC, abundance, Vulpes vulpes, detection/non-detection, red fox, spatial model},
	pages = {491--499}
}

@article{ruell_evaluation_2007,
	title = {Evaluation of {Noninvasive} {Genetic} {Sampling} {Methods} for {Felid} and {Canid} {Populations}},
	volume = {71},
	issn = {0022-541X},
	doi = {10.2193/2006-061},
	abstract = {Noninvasive sampling methods provide a means for studying species such as large mammalian carnivores that are difficult to survey using traditional techniques. Focusing on bobcat (Lynx rufus), we compared the effectiveness of noninvasive hair and scat genetic sampling in terms of field sample collection, species identification, and individual identification. We describe a novel hair-snare design and sampling protocol that successfully sampled 4 sympatric carnivore species, bobcat, mountain lion (Felis concolor), coyote (Canis latrans), and gray fox (Urocyon cinereoargenteus), in 3 habitat blocks in coastal southern California, USA. Scat surveys were also successful at sampling bobcats and other carnivores in the area. Hair and scat sampling methods had similar species identification success (81\% and 87\%, respectively) using mitochondrial DNA amplification and restriction enzyme digestion patterns. Therefore, for studies focused on the distribution and activity of a suite of carnivore species, we recommend a combination of noninvasive methodologies, for example, targeting hair and scat surveys toward species and sites where they are most effective. Because of a higher success rate for scat (85\%) than for hair samples (10\%) when using 4 microsatellite loci and a multiple-tubes approach to verify individual genotypes, we suggest scat sampling is a better choice for studies that require individual identification of bobcat},
	number = {5},
	journal = {The Journal of Wildlife Management},
	author = {Ruell, E W and Crooks, K R},
	year = {2007},
	note = {Citation Key: Ruell2007a
ISBN: 0022-541X},
	keywords = {bobcat, Lynx rufus, noninvasive sampling, mountain lion, microsatellites, coyote, gray fox, hair snare, mitochondrial DNA, scat.},
	pages = {1690--1694},
	annote = {Qiagen QIAamp DNA Stool Mini Kit}
}

