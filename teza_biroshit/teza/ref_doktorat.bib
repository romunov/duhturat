@article{nilsen_can_2008,
	title = {Can minimum convex polygon home ranges be used to draw biologically meaningful conclusions?},
	volume = {23},
	issn = {09123814},
	url = {http://link.springer.com/10.1007/s11284-007-0421-9},
	doi = {10.1007/s11284-007-0421-9},
	abstract = {Many conclusions about mammalian ranging behaviour have been drawn based on minimum convex polygon (MCP) estimates of home range size, although several studies have revealed its unpredictable nature compared to that of the kernel density estimator. We investigated to what extent the choice of home range estimator affected the biological interpretation in comparative studies. We found no discrepancy when the question asked covered a wide range of taxa, as real and very large differences in range size were likely to have masked smaller differences due to the choice of home range estimator. However, when the question asked concerned within-species characteristics, the choice of home range estimator explained as much of the variation in range size as did the ecological variable in question. The implications for macro-ecological and intraspecific studies are discussed.},
