@article{wilson_scale_2015,
	title = {Scale dependence in occupancy models: implications for estimating bear den distribution and abundance},
	volume = {6},
	issn = {2150-8925},
	url = {http://doi.wiley.com/10.1890/ES15-00250.1},
	doi = {10.1890/ES15-00250.1},
	abstract = {Monitoring programs are typically designed to identify long-term trends{\textbackslash}nin animal abundance, however estimating abundance at a relevant scale{\textbackslash}ncan be logistically prohibitive. This is particularly true for species{\textbackslash}nthat occur at low densities or those with large home ranges. In such{\textbackslash}ncases, occupancy surveys are often employed in place of more expensive{\textbackslash}nabundance estimation techniques such as mark-recapture because precise{\textbackslash}nestimation of occupancy probability generally requires fewer data.{\textbackslash}nAlthough choice of plot size is a critical design element of occupancy{\textbackslash}nmonitoring, relatively little effort has been expended to develop or{\textbackslash}ntest plot size recommendations. Animal movement between surveys can{\textbackslash}ncomplicate efforts to obtain an optimal plot size, but surveys of fixed{\textbackslash}nobjects, such as nests, dens, or burrows can provide insight about scale{\textbackslash}neffects because the population exposed to sampling does not change{\textbackslash}nduring the duration of the survey. We used repeated aerial occupancy{\textbackslash}nsurveys to obtain estimates of brown bear (Ursus arctos) den{\textbackslash}ndistribution and abundance in a portion of Katmai National Park and{\textbackslash}nPreserve in Alaska. We then used these data to assess the importance of{\textbackslash}nplot size selection and highlight the effects of spatial grain on the{\textbackslash}nresulting inference and utility for monitoring. Scale effects in{\textbackslash}nestimates of mean den-based site occupancy, but not total den abundance{\textbackslash}ndemonstrated that careful selection of sample unit size is important if{\textbackslash}nestimating occupancy probability is a primary monitoring objective. We{\textbackslash}nexpect occupancy surveys based on important structures such as nests or{\textbackslash}ndens could have wide applicability for many species.},
	number = {9},
	journal = {Ecosphere},
	author = {Wilson, Tammy L and Schmidt, Joshua H},
	month = sep,
	year = {2015},
	note = {Citation Key: Wilson2015},
	keywords = {ursus arctos, abundance monitoring, accepted 1 may 2015, aerial survey, alaska, article distributed under the, c, copyright, corresponding editor, d, grain, habitat maps, katmai national park and, ó 2015 wilson and, p, peters, preserve, published 29 september 2015, received 28 april 2015, schmidt, spatial, species distribution monitoring, terms of the creative, this is an open-access},
