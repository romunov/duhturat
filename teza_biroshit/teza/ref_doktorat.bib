	note = {Citation Key: mondoletal2009
ISBN: 0006-3207},
	keywords = {Elusive carnivores, Individual identification, Population estimation, Carnivore conservation, Fecal DNA, Genetic capture-recapture sampling, Panthera tigris, Scats},
	pages = {2350--2360},
