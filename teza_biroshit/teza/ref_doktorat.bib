	note = {Citation Key: Bousquet2008
ISBN: 1398-9995 (Electronic){\textbackslash}r0105-4538 (Linking)},
	keywords = {Specificity, Immunoglobulin E, Predictive values, Sensibility, Sensitization, Skin prick test},
