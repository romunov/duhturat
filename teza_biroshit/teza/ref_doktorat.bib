@article{marucco_bridging_2011,
	title = {Bridging the gaps between non-invasive genetic sampling and population parameter estimation},
	volume = {57},
	issn = {16124642},
	doi = {10.1007/s10344-010-0477-7},
	abstract = {Reliable estimates of population parameters are necessary for effective management and conservation actions. The use of genetic data for capture–recapture (CR) analyses has become an important tool to estimate population parameters for elusive species. Strong emphasis has been placed on the genetic analysis of non-invasive samples, or on the CR analysis; however, little attention has been paid to the simultaneous overview of the full non- invasive genetic CR analysis, and the important insights gained by understanding the interactions between the different parts of the technique. Here, we review the three main steps of the approach: designing the appropriate sampling scheme, conducting the genetic lab analysis, and applying the CR analysis to the genetic results; and present a synthesis of this topic with the aim of discussing the primary limitations and sources of error. We discuss the importance of the integration between these steps, the unique situations which occur with non-invasive studies, the role of ecologists and geneticists throughout the process, the problem of error propagation, and the sources of biases which can be present in the final estimates. We highlight the importance of team collaboration and offer a series of recommendations to wildlife ecologists who are not familiar with this topic yet but may want to use this tool to monitor populations through time.},
	number = {1},
	journal = {European Journal of Wildlife Research},
	author = {Marucco, Francesca and Boitani, Luigi and Pletscher, Daniel H. and Schwartz, Michael K.},
	year = {2011},
	note = {Citation Key: marucco-et-al-2013
ISBN: 1612-4642},
	keywords = {Population size, Capture-mark-recapture, Genetic, Molecular tagging, Non-invasive},
	pages = {1--13}
}

@article{barroso_eurythoe_2009,
	title = {\textit{{Eurythoe} complanata} ({Polychaeta}: {Amphinomidae}), the 'cosmopolitan' fireworm, consists of at least three cryptic species},
