@article{peron_estimation_2013,
	title = {Estimation of bird and bat mortality at wind-power farms with superpopulation models},
	volume = {50},
	issn = {00218901},
	doi = {10.1111/1365-2664.12100},
	abstract = {1. Collision of birds and bats with turbines in utility-scale wind farms{\textbackslash}nis an increasing cause of concern. 2. Carcass counts conducted to{\textbackslash}nquantify the take' of protected species need to be corrected for carcass{\textbackslash}npersistence probability (removal by scavengers and decay) and detection{\textbackslash}nprobability (searcher efficiency). These probabilities may vary with{\textbackslash}ntime since death, because of intrinsic changes in carcass properties{\textbackslash}nwith age and of heterogeneity (preferential removal of easy-to-detect{\textbackslash}ncarcasses). 3. In this article, we describe the use of superpopulation{\textbackslash}ncapture-recapture models to perform the required corrections to the raw{\textbackslash}ncount data. We review how to make such models age specific and how to{\textbackslash}ncombine trial experiments with carcass searches in order to accommodate{\textbackslash}nthe fact that carcasses are stationary (which affects the detection{\textbackslash}nprocess). 4. We derive information about optimal sampling design{\textbackslash}n(proportion of the turbines to sample, number of sampling occasions,{\textbackslash}ninterval between sampling occasions) and use simulations to illustrate{\textbackslash}nthe expected precision of mortality estimates. We analyse data from a{\textbackslash}nsmall wind farm in New Jersey, in which we find the estimated number of{\textbackslash}nfatalities to be twice the number of carcasses found. 5. Synthesis and{\textbackslash}napplications. Our approach to estimation of wind farm mortality based on{\textbackslash}ndata from carcass surveys is flexible and can accommodate a range of{\textbackslash}ndifferent sampling designs and biological hypotheses. Resulting{\textbackslash}nmortality estimates can be used (1) to quantify the required amount of{\textbackslash}ncompensation actions, (2) to inform mortality projections for proposed{\textbackslash}nwind development sites and (3) to inform decisions about management of{\textbackslash}nexisting wind farms.},
	number = {4},
	journal = {Journal of Applied Ecology},
	author = {Péron, Guillaume and Hines, James E. and Nichols, James D. and Kendall, William L. and Peters, Kimberly A. and Mizrahi, David S.},
	year = {2013},
	note = {Citation Key: peron-et-al-2013
ISBN: 00218901},
	keywords = {Superpopulation, Bioeconomic models, Carcass search, Eastern red bat, Golden eagle, Hoary bat, Optimal sampling design, Wildlife fatalities, Wind power, Wind turbines},
	pages = {902--911}
}

@techreport{solana-gutierrez_bayesian_2014,
	title = {A {Bayesian} state-space model for estimating wild boar dynamic population in a hunting estate},
	abstract = {This paper deals with a Bayesian state-space model to estimate wild boar abundance and demographic parameters such as survival rates, intrinsic growth rates and hunting efficiency for a Mediterranean hunting estate. Based on annual wild boar captures from 1995 to 2008, a Bayesian model is built where population abundance is estimated by a reverse-time multinomial Cormack-JollySeber model. In this model, monthly maximum and minimum temperature average, also droughtconstrained, frost-constrained and non-constrained bioclimatic intensity are used as predictive factors of the population birth-rate and intrinsic growth. As a result, a wild board population dynamic model was obtained which simulates hunting activities and estimates hidden demographic parameters. These outcomes are useful to develop better hunting management plans, where future captures are determined according to actual climatic features.},
	author = {Solana-Gutiérrez, J and Thomas, L},
	year = {2014},
	note = {Citation Key: solana-gutierrez-thomas-2014},
	keywords = {State-space model, Bayesian model, Reverse-time CJS model, Wild boar hunting},
	pages = {1--4}
}

@article{fumi_distinguishing_2008,
	title = {Distinguishing between \textit{{Leptidea} sinapis} and \textit{{L}. reali} ({Lepidoptera}: {Pieridae}) using a morphometric approach: {Impact} of measurement error on the discriminative characters},
	volume = {1819},
	issn = {11755326},
	abstract = {A morphometric approach was used to test the possibility of discriminating between L. sinapis and L. reali by taking into account some new genitalic characters in addition to those used in previous surveys. Principal component analysis, performed on the size-and-shape data sets and on the size-adjusted data sets, has allowed two completely separate morphotypes to be detected, both in males and in females. Discriminant analysis has confirmed the separation of previously detected morphotypes and has correctly classified 100\% of the specimens in both sexes with six discriminative characters being identified in males and two in females. However, some of these discriminative characters were not considered reliable enough because of the high associated measurement error and the scarce discriminative power. Reliable discriminative characters were: vinculum (≈ valve) width, length of phallus (≈ aedoeagus ≈ aedeagus), saccus and uncus in males and ductus bursae length in females. The main topics discussed are: a comparison of the discriminative characters with previous studies, the sources of measurement error and the devices used to reduce it, as well as the between and within-species variability of the characters.},
	number = {1819},
	journal = {Zootaxa},
	author = {Fumi, Michele},
	year = {2008},
	note = {Citation Key: fumi2008},
	keywords = {Discriminant analysis, Principal component analysis, Repeatability, Sibling species, Size-adjustments},
	pages = {40--54}
}

@article{augustine_spatial_2016,
	title = {Spatial capture-recapture with partial identity: {An} application to camera traps},
	url = {http://biorxiv.org/lookup/doi/10.1101/056804},
	doi = {10.1101/056804},
	abstract = {Camera trapping surveys frequently capture individuals whose identity is only known from a single flank. The most widely used methods for incorporating these partial identity individuals into density analyses do not use all of the partial identity capture histories, reducing precision, and while not previously recognized, introducing bias. Here, we present the spatial partial identity model (SPIM), which uses the spatial location where partial identity samples are captured to probabilistically resolve their complete identities, allowing all partial identity samples to be used in the analysis. We show that the SPIM out-performs other analytical alternatives. We then apply the SPIM to an ocelot data set collected on a trapping array with double-camera stations and a bobcat data set collected on a trapping array with single-camera stations. The SPIM improves inference in both cases and in the ocelot example, individual sex determined from photographs is used to further resolve partial identities, one of which is resolved to near certainty. The SPIM opens the door for the investigation of trapping designs that deviate from the standard 2 camera design, the combination of other data types between which identities cannot be deterministically linked, and can be extended to the problem of partial genotypes.},
	journal = {bioRxiv},
	author = {Augustine, Ben C and Royle, J Andrew and Kelly, Marcella J and Satter, Christopher B and Alonso, Robert S and Crooks, Kevin R},
	month = jun,
	year = {2016},
	note = {arXiv: http://biorxiv.org/content/early/2016/06/03/056804
Citation Key: Augustine2016},
	keywords = {secr, camera trapping, multiple marks, partial identity, spatial capture-recapture}
}

