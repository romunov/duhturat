ISBN: 1932-6203},
	keywords = {Wolves, Inbreeding, Population, Genetics, Animals, Endangered Species, Female, Genetic, Heterozygote, Linkage Disequilibrium, Male, Microsatellite Repeats, Scandinavia, Selection, Wild, Wild: genetics, Wolves: genetics},
	pages = {e72}
}

@article{pledger_unified_2000,
