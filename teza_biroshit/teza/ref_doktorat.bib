	journal = {Journal of Agricultural, Biological, and Environmental Statistics},
	author = {Bailey, R. A.},
	year = {2012},
	note = {Citation Key: bailey-2012},
	keywords = {Restricted randomization, Row-column designs, Super-valid randomization},
	pages = {176--191}
}

