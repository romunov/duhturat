	note = {Citation Key: Wehausen2004a
ISBN: 0022-1503},
	pages = {503--509},
	annote = {Qiagen stool minikits}
}

@article{gerloff_amplification_1995,
	title = {Amplification of hypervariable simple sequence repeats (microsatellites) from excremental {DNA} of wild living bonobos (\textit{{Pan} paniscus})},
